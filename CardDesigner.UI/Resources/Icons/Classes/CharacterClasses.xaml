<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Viewbox x:Key="Druid">
        <Canvas
            Width="100"
            Height="100">
            <Ellipse
                Canvas.Left="25"
                Canvas.Top="25"
                Width="50"
                Height="50"
                Fill="Red"
                Stroke="Yellow"
                StrokeThickness="2" />
        </Canvas>
    </Viewbox>
</ResourceDictionary>